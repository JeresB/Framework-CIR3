{% extends 'BackOfficeBundle:Default:layout.html.twig' %}
{% block content %}
    <table class="table table-bordered">
        <thead>
            <tr>
            <th>#</th>
            <th>Libelle</th>
            <th>Value</th>
            <th>NB</th>
            </tr>
        </thead>
        </tbody>
            {% for i in 0..10 %}
                <tr><td>{{ i }}</td><td>{{ libelle }}{{ i }}</td><td>{{ value }}{{ i }}</td><td>{{ nb }}{{ i }}</td></tr>
            {% endfor %}
        </tbody>
    </table>

{% endblock %}